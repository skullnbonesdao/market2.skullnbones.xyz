{
  "openapi": "3.0.3",
  "info": {
    "title": "RogueMarketAPI",
    "description": "This API is used by the RogueMarket to display charts and data",
    "license": {
      "name": "MIT"
    },
    "version": "0.3.0"
  },
  "paths": {
    "/get_cursor": {
      "get": {
        "tags": [
          "cursor"
        ],
        "operationId": "get_cursor",
        "responses": {
          "200": {
            "description": "Request cursors",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CursorBlock"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/get_price_change": {
      "get": {
        "tags": [
          "trades"
        ],
        "operationId": "get_price_change",
        "parameters": [
          {
            "name": "mint_currency",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "example": "ATLASXmbPQxBUYbxPsV97usA3fPQYEqzQBUHgiFCUsXx"
          },
          {
            "name": "mint_asset",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "example": "foodQJAztMzX1DKpLaiounNe2BDMds5RNuPC6jsNrDG"
          },
          {
            "name": "timespan",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "day",
                "week",
                "month",
                "year"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Get current market price changes",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/PriceChange"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/get_prices": {
      "get": {
        "tags": [
          "trades"
        ],
        "operationId": "get_prices",
        "parameters": [
          {
            "name": "mint_currency",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "example": "ATLASXmbPQxBUYbxPsV97usA3fPQYEqzQBUHgiFCUsXx"
          },
          {
            "name": "mint_asset",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "nullable": true
            },
            "example": "tooLsNYLiVqzg8o4m3L2Uetbn62mvMWRqkog6PQeYKL"
          }
        ],
        "responses": {
          "200": {
            "description": "Get prices for currency",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Price"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/get_trades": {
      "get": {
        "tags": [
          "trades"
        ],
        "operationId": "get_trades",
        "parameters": [
          {
            "name": "mint_currency",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "nullable": true
            }
          },
          {
            "name": "mint_asset",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "nullable": true
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "nullable": true,
              "minimum": 0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List todos successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Trade"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/search": {
      "get": {
        "tags": [
          "trades"
        ],
        "operationId": "search",
        "parameters": [
          {
            "name": "search_option",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "signature",
                "asset",
                "currency",
                "maker",
                "taker",
                "maker_and_taker"
              ]
            }
          },
          {
            "name": "search",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "example": "foodQJAztMzX1DKpLaiounNe2BDMds5RNuPC6jsNrDG"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32",
              "nullable": true,
              "minimum": 0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Get current market price changes",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Trade"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/udf/": {
      "get": {
        "tags": [
          "udf"
        ],
        "summary": "UDF Home",
        "description": "UDF Home\n\nResponses with server time.",
        "operationId": "get_home",
        "responses": {
          "200": {
            "description": "Get time successfully",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/udf/config": {
      "get": {
        "tags": [
          "udf"
        ],
        "summary": "Get UDF-Config",
        "description": "Get UDF-Config\n\nResponses with a UDF config in json.",
        "operationId": "get_config",
        "responses": {
          "200": {
            "description": "Response: Config successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/UdfConfig"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/udf/history": {
      "get": {
        "tags": [
          "udf"
        ],
        "summary": "Get History request",
        "description": "Get History request\n\nResponses with a History-Result in json (oclh).",
        "operationId": "get_history",
        "parameters": [
          {
            "name": "symbol",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "style": "form",
            "example": "FOODATLAS"
          },
          {
            "name": "from",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int64",
              "nullable": true
            },
            "style": "form",
            "example": "1677799981"
          },
          {
            "name": "to",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int64",
              "nullable": true
            },
            "style": "form",
            "example": "1678663981"
          },
          {
            "name": "resolution",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "nullable": true
            }
          },
          {
            "name": "countback",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int64",
              "nullable": true,
              "minimum": 0
            }
          },
          {
            "name": "currencyCode",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "nullable": true
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Response: SymbolInfo successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/UdfHistory"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/udf/search": {
      "get": {
        "tags": [
          "udf"
        ],
        "summary": "Get Search request",
        "description": "Get Search request\n\nResponses with a Search-Result in json.",
        "operationId": "get_search",
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "style": "form",
            "example": "SymbolsParams"
          },
          {
            "name": "type",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "nullable": true
            }
          },
          {
            "name": "exchange",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "nullable": true
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "minimum": 0
            },
            "style": "form",
            "example": "2"
          }
        ],
        "responses": {
          "200": {
            "description": "Response: SymbolInfo successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/UdfSearchSymbol"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/udf/symbol_info": {
      "get": {
        "tags": [
          "udf"
        ],
        "summary": "Get Symbol group request",
        "description": "Get Symbol group request\n\nResponses with a SymbolGroup in json.",
        "operationId": "get_symbol_info",
        "parameters": [
          {
            "name": "group",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "nullable": true
            },
            "style": "form",
            "example": "FOOD"
          }
        ],
        "responses": {
          "200": {
            "description": "Response: SymbolInfo successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/UdfSymbolInfo"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/udf/symbols": {
      "get": {
        "tags": [
          "udf"
        ],
        "summary": "Get Symbol resolve request",
        "description": "Get Symbol resolve request\n\nResponses with a SymbolInfo in json.",
        "operationId": "get_symbols",
        "parameters": [
          {
            "name": "symbol",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "style": "form",
            "example": "FOODATLAS"
          }
        ],
        "responses": {
          "200": {
            "description": "Response: SymbolInfo successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/UdfSymbolInfo"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Nothing found"
          }
        }
      }
    },
    "/udf/time": {
      "get": {
        "tags": [
          "udf"
        ],
        "summary": "Get ServerTime",
        "description": "Get ServerTime\n\nResponses with server time.",
        "operationId": "get_time",
        "responses": {
          "200": {
            "description": "Response: Time successful",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CursorBlock": {
        "type": "object",
        "required": [
          "block_num"
        ],
        "properties": {
          "block_num": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "Exchange": {
        "type": "object",
        "required": [
          "value",
          "name",
          "desc"
        ],
        "properties": {
          "desc": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      },
      "Price": {
        "type": "object",
        "properties": {
          "asset": {
            "type": "string",
            "nullable": true
          },
          "currency": {
            "type": "string",
            "nullable": true
          },
          "price": {
            "type": "number",
            "format": "double",
            "nullable": true
          }
        }
      },
      "PriceChange": {
        "type": "object",
        "properties": {
          "close_price": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "open_price": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "price_change_percentage": {
            "type": "number",
            "format": "double",
            "nullable": true
          }
        }
      },
      "SymbolsType": {
        "type": "object",
        "required": [
          "value",
          "name"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      },
      "Trade": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "asset": {
            "type": "string",
            "nullable": true
          },
          "block": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "currency": {
            "type": "string",
            "nullable": true
          },
          "fee": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "id": {
            "type": "string"
          },
          "maker": {
            "type": "string",
            "nullable": true
          },
          "price": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "seller": {
            "type": "string",
            "nullable": true
          },
          "side": {
            "type": "string",
            "nullable": true
          },
          "signature": {
            "type": "string",
            "nullable": true
          },
          "size": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "taker": {
            "type": "string",
            "nullable": true
          },
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "volume": {
            "type": "number",
            "format": "double",
            "nullable": true
          }
        }
      },
      "UdfConfig": {
        "type": "object",
        "required": [
          "exchanges",
          "symbols_types",
          "supported_resolutions",
          "supports_search",
          "supports_group_request",
          "supports_marks",
          "supports_timescale_marks",
          "supports_time"
        ],
        "properties": {
          "exchanges": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Exchange"
            }
          },
          "supported_resolutions": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "supports_group_request": {
            "type": "boolean"
          },
          "supports_marks": {
            "type": "boolean"
          },
          "supports_search": {
            "type": "boolean"
          },
          "supports_time": {
            "type": "boolean"
          },
          "supports_timescale_marks": {
            "type": "boolean"
          },
          "symbols_types": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SymbolsType"
            }
          }
        }
      },
      "UdfHistory": {
        "type": "object",
        "required": [
          "s",
          "t",
          "c",
          "o",
          "h",
          "l",
          "v"
        ],
        "properties": {
          "c": {
            "type": "array",
            "items": {
              "type": "number",
              "format": "double",
              "nullable": true
            }
          },
          "h": {
            "type": "array",
            "items": {
              "type": "number",
              "format": "double",
              "nullable": true
            }
          },
          "l": {
            "type": "array",
            "items": {
              "type": "number",
              "format": "double",
              "nullable": true
            }
          },
          "o": {
            "type": "array",
            "items": {
              "type": "number",
              "format": "double",
              "nullable": true
            }
          },
          "s": {
            "type": "string"
          },
          "t": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int64"
            }
          },
          "v": {
            "type": "array",
            "items": {
              "type": "number",
              "format": "double",
              "nullable": true
            }
          }
        }
      },
      "UdfSearchSymbol": {
        "type": "object",
        "required": [
          "symbol",
          "full_name",
          "description",
          "exchange",
          "ticker",
          "type"
        ],
        "properties": {
          "description": {
            "type": "string"
          },
          "exchange": {
            "type": "string"
          },
          "full_name": {
            "type": "string"
          },
          "symbol": {
            "type": "string"
          },
          "ticker": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      },
      "UdfSymbolInfo": {
        "type": "object",
        "required": [
          "symbol",
          "ticker",
          "name",
          "full_name",
          "description",
          "exchange",
          "listed_exchange",
          "type",
          "currency_code",
          "session",
          "timezone",
          "minmovement",
          "minmov",
          "minmovement2",
          "minmov2",
          "pricescale",
          "supported_resolutions",
          "has_intraday",
          "has_daily",
          "has_weekly_and_monthly",
          "data_status"
        ],
        "properties": {
          "currency_code": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "data_status": {
            "type": "string"
          },
          "description": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "exchange": {
            "type": "string"
          },
          "full_name": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "has_daily": {
            "type": "boolean"
          },
          "has_intraday": {
            "type": "boolean"
          },
          "has_weekly_and_monthly": {
            "type": "boolean"
          },
          "listed_exchange": {
            "type": "string"
          },
          "minmov": {
            "type": "number",
            "format": "double"
          },
          "minmov2": {
            "type": "number",
            "format": "double"
          },
          "minmovement": {
            "type": "number",
            "format": "double"
          },
          "minmovement2": {
            "type": "number",
            "format": "double"
          },
          "name": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "pricescale": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int64"
            }
          },
          "session": {
            "type": "string"
          },
          "supported_resolutions": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "symbol": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "ticker": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "timezone": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "api_key": {
        "type": "apiKey",
        "in": "header",
        "name": "todo_apikey"
      }
    }
  },
  "tags": [
    {
      "name": "trades",
      "description": "Get recent Trades"
    },
    {
      "name": "udf",
      "description": "Request data in UDF format!"
    }
  ]
}
